    "authentication",
    "user-management",
    "realms",
    "identity-management",
    "iam"
  ],
  "author": "M0-AR <mohamad.ashmar.ai@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/M0-AR/keycloak-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/M0-AR/keycloak-mcp-server/issues"
  },
  "homepage": "https://github.com/M0-AR/keycloak-mcp-server#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@keycloak/keycloak-admin-client": "^25.0.0",
